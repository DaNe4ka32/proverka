{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",  // ← указать правильный путь
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"     // ← папка dist внутри frontend
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
